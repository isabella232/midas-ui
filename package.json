{
  "name": "midas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "start:local": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:debug": "DEBUG_PRINT_LIMIT=40000 react-scripts test --coverage",
    "test:all": "react-scripts test --coverage --watchAll=false",
    "test:summary": "react-scripts test --coverage --coverageReporters=\"text-summary\"",
    "test:lite": "react-scripts test --bail",
    "test:unit": "react-scripts test --coverage --watchAll=false --runInBand",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint src/.",
    "lint:fix": "eslint src/. --fix",
    "eject": "react-scripts eject",
    "test:e2e": "npx cypress run",
    "test:e2e-ci": "npx cypress run --env configFile=pipeline --config integrationFolder=cypress/integration,pluginsFile=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/Styles/materialThemes.js"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.52",
    "@mui/material": "^5.0.5",
    "@mui/styles": "^5.0.2",
    "@reduxjs/toolkit": "^1.6.1",
    "@stomp/stompjs": "^6.1.2",
    "ansi-html": "https://registry.yarnpkg.com/ansi-html-community/-/ansi-html-community-0.0.8.tgz#69fbc4d6ccbe383f9736934ae34c3f8290f1bf41",
    "axios": "^0.22.0",
    "date-fns": "^2.25.0",
    "js-file-download": "^0.4.12",
    "object-hash": "^2.2.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-minimal-pie-chart": "^8.2.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.1",
    "validator": "^13.7.0"
  },
  "resolutions": {
    "set-value": "^4.0.1",
    "immer": "^9.0.6",
    "ansi-html": "https://registry.yarnpkg.com/ansi-html-community/-/ansi-html-community-0.0.8.tgz#69fbc4d6ccbe383f9736934ae34c3f8290f1bf41",
    "ansi-regex": "^5.0.1",
    "tmpl": "^1.0.5",
    "nth-check": "^2.0.1",
    "browserslist": "^4.16.5",
    "glob-parent": "^5.1.2",
    "tar": "^4.4.18"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-transform-typescript": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@cypress/react": "^5.9.4",
    "@cypress/webpack-dev-server": "^1.5.0",
    "@testing-library/dom": "^8.7.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.9",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/parser": "^5.0.0",
    "agent-base": "^6.0.2",
    "cypress": "^9.0.1",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-xss": "^0.1.10",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.4.4"
  }
}
