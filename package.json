{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:local": "rm -rf ./server/build && react-scripts build && mv ./build ./server/",
    "build:zip": "./scripts/build-zip.sh",
    "test": "react-scripts test --coverage",
    "test:all": "npm test -- --coverage --watchAll=false",
    "test:summary": "react-scripts test --coverage --coverageReporters=\"text-summary\"",
    "test:lite": "react-scripts test --bail",
    "test:unit": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint src/.",
    "lint:fix": "eslint src/. --fix",
    "eject": "react-scripts eject",
    "e2e:local": "npx cypress open --env INTEGRATION_TEST_HOST=http://localhost:9000/",
    "test:e2e-ci": "npx cypress run --env INTEGRATION_TEST_HOST=https://internet-site/ --browser electron"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/Styles/materialThemes.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "redux": "^4.0.5"
  },
  "resolutions": {
    "faye-websocket": "^0.11.3",
    "immer": "^8.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.1.3",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "stylelint": "^13.10.0"
  }
}
