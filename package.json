{
  "name": "midas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "start:local": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:debug": "DEBUG_PRINT_LIMIT=40000 react-scripts test --coverage",
    "test:all": "react-scripts test --coverage --watchAll=false",
    "test:summary": "react-scripts test --coverage --coverageReporters=\"text-summary\"",
    "test:lite": "react-scripts test --bail",
    "test:unit": "react-scripts test --coverage --watchAll=false --runInBand",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint src/.",
    "lint:fix": "eslint src/. --fix",
    "eject": "react-scripts eject",
    "test:e2e": "npx cypress run",
    "test:e2e-ci": "npx cypress run --env configFile=pipeline --config integrationFolder=cypress/integration,pluginsFile=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/Styles/materialThemes.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.4",
    "@reduxjs/toolkit": "^1.6.1",
    "@stomp/stompjs": "^6.1.1",
    "axios": "^0.21.4",
    "object-hash": "^2.2.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-minimal-pie-chart": "^8.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.1",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.1",
    "validator": "^13.6.0"
  },
  "resolutions": {
    "browserslist": "^4.16.5",
    "glob-parent": "^5.1.2",
    "tar": "^4.4.18"
  },
  "devDependencies": {
    "@cypress/react": "^5.9.4",
    "@cypress/webpack-dev-server": "^1.5.0",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-xss": "^0.1.10",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "stylelint": "^13.13.1"
  }
}
