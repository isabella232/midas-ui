{
  "name": "midas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "start:local": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:debug": "DEBUG_PRINT_LIMIT=40000 react-scripts test --coverage",
    "test:all": "react-scripts test --coverage --watchAll=false",
    "test:summary": "react-scripts test --coverage --coverageReporters=\"text-summary\"",
    "test:lite": "react-scripts test --no-coverage",
    "test:unit": "react-scripts test --coverage --watchAll=false --runInBand",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint src/.",
    "lint:fix": "eslint src/. --fix",
    "eject": "react-scripts eject",
    "test:e2e": "npx cypress run",
    "test:e2e-ci": "npx cypress run --env configFile=pipeline --config integrationFolder=cypress/integration,pluginsFile=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/Styles/materialThemes.js"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.4",
    "@mui/lab": "^5.0.0-alpha.60",
    "@mui/material": "^5.2.4",
    "@mui/styles": "^5.2.3",
    "@reduxjs/toolkit": "^1.6.1",
    "@stomp/stompjs": "^6.1.2",
    "axios": "^0.26.0",
    "date-fns": "^2.27.0",
    "js-file-download": "^0.4.12",
    "object-hash": "^2.2.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-minimal-pie-chart": "^8.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "5.3.0",
    "react-scripts": "^5.0.0",
    "redux": "^4.1.2",
    "sockjs-client": "^1.5.2",
    "validator": "^13.7.0"
  },
  "resolutions": {
    "follow-redirects": "^1.14.8",
    "nth-check": "^2.0.1",
    "node-forge": "^1.0.0",
    "postcss": "^8.2.13",
    "url-parse": "^1.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@cypress/react": "^5.11.0",
    "@cypress/webpack-dev-server": "^1.8.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/parser": "^5.7.0",
    "agent-base": "^6.0.2",
    "cypress": "^9.1.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-xss": "^0.1.11",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.1",
    "stylelint": "^14.1.0",
    "typescript": "^4.5.4"
  }
}
